<p id="notice"><%= notice %></p>

<h1>Employeers</h1>

<table>
  <thead>
    <tr>
      <th>Company name</th>
      <th>Company name url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employeers.each do |employeer| %>
      <tr>
        <td><%= employeer.company_name %></td>
        <td><%= employeer.company_name_url %></td>
        <td><%= link_to 'Show', employeer %></td>
        <td><%= link_to 'Edit', edit_employeer_path(employeer) %></td>
        <td><%= link_to 'Destroy', employeer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employeer', new_employeer_path %>
